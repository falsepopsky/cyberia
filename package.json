{
  "name": "cyberia",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.0",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.15.0"
}
